@font-face
    font-family: 'Almendra'
    font-style: italic
    font-weight: 400
    font-display: swap
    src: url(https://fonts.gstatic.com/s/almendra/v15/H4ciBXKAlMnTn0CskxY4yLs.ttf) format('truetype')

@font-face
    font-family: 'Almendra'
    font-style: italic
    font-weight: 700
    font-display: swap
    src: url(https://fonts.gstatic.com/s/almendra/v15/H4chBXKAlMnTn0CskxY48Ae9oqY.ttf) format('truetype')

span
    &.spell
        font-family: 'Almendra', serif!important
        letter-spacing: .015em
        font-size: 1.1em
        line-height: 1.3

@media (min-width: 1200px)
    [character]
        p
            & > span
                &.spell
                    font-size: 1.15em

